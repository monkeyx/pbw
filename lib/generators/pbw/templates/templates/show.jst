<table id="<%= singular_name %>-table">
	<tbody>
		<tr>
			<th>Name:</th>
			<td><%%= name %></td>
		</tr>
		<% attributes.each do |attribute| -%>
		<tr>
		  <th><%= attribute.human_name %>:</th>
		  <td><%%= <%= attribute.name %> %></td>
		</tr>
		<% end -%>
		
	</tbody>
</table>
<a href="#/<%=plural_model_name%>">Back</a>